{
	"name": "qsp-syntax-obsidian",
	"version": "1.1.0",
	"description": "This plugin add to your Obsidian storage highlighting of QSP-syntax. (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"qsp"
	],
	"author": {
		"name": "Aleks Versus",
		"email": "lex666endless@gmail.com",
		"url": "https://github.com/typhoon-kim"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AleksVersus/qsp-syntax-obsidian"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@codemirror/state": "^6.4.1",
		"@codemirror/view": "^6.26.3"
	}
}
